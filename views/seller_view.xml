<odoo>
    <record id="view_form_seller" model="ir.ui.view">
        <field name="name">Seller Form</field>
        <field name="model">tiktok.seller</field>
        <field name="arch" type="xml">
            <form>
                <field name="name" />
                <field name="email" />
            </form>
        </field>
    </record>

    <record id="view_tree_seller" model="ir.ui.view">
        <field name="name"></field>      
        <field name="model"></field>
        <field name="arch" type="xml">
            <tree>
            </tree>
        </field>
    </record>

    <record id="action_tiktok_seller" model="ir.actions.act_window">
        <field name="name">Seller Management</field>
        <field name="res_model">tiktok.seller</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_tiktok_seller"
        name="Seller Management"
        parent="menu_tiktok"
        action="action_titkok_seller"
        sequence="80"/>
</odoo>
